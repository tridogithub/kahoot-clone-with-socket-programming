<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import org.controlsfx.control.HiddenSidesPane?>

<?import javafx.scene.image.Image?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.EndGameController">
    <children>
        <Label layoutX="222.0" layoutY="64.0" prefHeight="30.0" prefWidth="167.0" text="Your Total Point">
            <font>
                <Font size="20.0"/>
            </font>
        </Label>
        <Label fx:id="btnPoint" alignment="CENTER" layoutX="253.0" layoutY="165.0" prefHeight="70.0" prefWidth="93.0"
               text="10" textAlignment="CENTER">
            <font>
                <Font size="41.0"/>
            </font>
        </Label>
        <Button fx:id="btnHistoryAns" layoutX="153.0" layoutY="279.0" mnemonicParsing="false"
                text="View History Answer"/>
        <Button fx:id="btnRankTable" layoutX="349.0" layoutY="279.0" mnemonicParsing="false" text="Rank Table"/>
        <Label layoutX="283.0" layoutY="148.0" text="TriDV">
            <font>
                <Font name="System Bold" size="12.0"/>
            </font>
        </Label>
        <ImageView fx:id="imageViewAvatar" fitHeight="47.0" fitWidth="43.0" layoutX="278.0" layoutY="92.0"
                   pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../images/arrow-left.png"/>
            </image>
        </ImageView>
        <HiddenSidesPane/>
        <ImageView fx:id="imageViewHiddenOpen" fitHeight="30.0" fitWidth="33.0" layoutX="567.0" layoutY="185.0"
                   pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../images/arrow-left.png"/>
            </image>
        </ImageView>
    </children>
</AnchorPane>
