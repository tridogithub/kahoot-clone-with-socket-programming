<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.CreateQuestionSuiteController">
   <children>
      <TextField fx:id="txtFilePath" layoutX="91.0" layoutY="166.0" prefHeight="34.0" prefWidth="275.0" promptText="File Path" />
      <Label layoutX="181.0" layoutY="82.0" text="Upload your own question file">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Button fx:id="btnJoin" layoutX="241.0" layoutY="256.0" mnemonicParsing="false" onMouseClicked="#handleSaveBtnClick" prefHeight="34.0" prefWidth="137.0" stylesheets="@../styling/fullpackstyling.css" text="Save" />
      <Button fx:id="btnBack" layoutX="24.0" layoutY="24.0" mnemonicParsing="false" onMouseClicked="#handleBackBtnClick" text="Back" />
      <Label fx:id="labelMessage" layoutX="196.0" layoutY="200.0" prefHeight="21.0" prefWidth="228.0" textAlignment="CENTER" />
      <Button fx:id="btnChooseFile" layoutX="372.0" layoutY="166.0" mnemonicParsing="false" onMouseClicked="#handleChooseFileBtnClick" prefHeight="34.0" prefWidth="137.0" stylesheets="@../styling/fullpackstyling.css" text="Choose file" />
   </children>
</AnchorPane>
